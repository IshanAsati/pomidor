    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0"
  },
  "dependencies": {
    "workbox-window": "^7.0.0"
